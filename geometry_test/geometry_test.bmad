no_digested
parameter[p0c] = 1e6 / 3.3356e-3
parameter[geometry] = open
beginning[beta_a] = 10
beginning[beta_b] = 10

particle_start[z] = pi/2
particle_start[x] = 1

d: drift, l = 1
p1: patch, z_offset = 1, x_pitch = pi

b: sbend, l = 1, angle = 1, roll = 0.1, ref_tilt = -1, x_pitch = 0.1, y_pitch = 0.2
b2: sbend, l = 1, g = 0.1

p_line: line = (d, b, p1, --d, --b)
p2_line: line = (b2)

use, p_line, p2_line
