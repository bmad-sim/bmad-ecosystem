  d1_thickness = 1e-9, d2_thickness = 1.5e-9, tilt = 0.01, 
  x_offset = 0.001, x_pitch = 1e-4, y_offset = 0.002, y_pitch = 2e-4,
	curvature = {x0y2 =  1 / (2 *r0), x0y3 = 1 / (10*r0^2), x2y0 = 1 / (2*r0), x3y0 = -1 / (12*r0^3),
	x2y2 = 1 / (6*r0^3), elliptical_x = 0.05, elliptical_y = 0.08, elliptical_z = 0.1}

mirror1: mirror,
  x_offset = 0.001, x_pitch = 1e-4, y_offset = 0.002, y_pitch = 2e-4,
	curvature = {x0y2 =  1 / (2 *r0), x0y3 = 1 / (10*r0^2), x2y0 = 1 / (2*r0), x3y0 = -1 / (12*r0^3),
