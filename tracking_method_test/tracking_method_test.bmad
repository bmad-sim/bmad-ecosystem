l1[geometry] = open
l1[beta_a] = 10
l1[beta_b] = 10

!-----------
! Separate e_gun from rest of elements since e_gun shifts reference energy.

l2: line = (e_gun1)
l2[e_tot] = 1e6
l2[geometry] = open
l2[beta_a] = 10
l2[beta_b] = 10

