l3: line = (crystal1, crystal2, mirror1, multilayer_mirror1)

l3[e_tot] = 9.7e3
l3[geometry] = open
l3[particle] = photon

r0 = 10

crystal1: crystal, crystal_type = 'Si(444)', b_param = -1.1, tilt = 0.1, psi_angle = 0.11,
  x_offset = 0.001, x_pitch = 1e-4, y_offset = 0.002, y_pitch = 2e-4,
	curvature = {x0y2 =  1 / (2 *r0), x0y3 = 1 / (10*r0^2), x2y0 = 1 / (2*r0), x3y0 = -1 / (12*r0^3),
	x2y2 = 1 / (6*r0^3), spherical = 0.1}

crystal2: crystal, crystal_type = 'Diamond(111)', b_param = 1.1, tilt = 0.1, psi_angle = 0.11,
  x_offset = 0.001, x_pitch = 1e-4, y_offset = 0.002, y_pitch = 2e-4,
	curvature = {x0y2 =  1 / (2 *r0), x0y3 = 1 / (10*r0^2), x2y0 = 1 / (2*r0), x3y0 = -1 / (12*r0^3),
	x2y2 = 1 / (6*r0^3), spherical = 0.1}, thickness = 1e-3

