&tao_var
    v1_var%name = 'begfloor'
    default_step = 1e-4
    default_universe = '1'
    ix_min_var = 1
    ix_max_var = 3
    var(1:3)%ele_name  = 'beginning', 'beginning', 'beginning'
    var(1:3)%attribute = 'x_position', 'z_position', 'theta_position'

/


&tao_var
    v1_var%name = 'quad'
    default_step = 1e-4
    default_attribute = 'K1'
    search_for_lat_eles = "quad::*"
    default_key_bound = T
    default_key_delta = 1e-3
/


&tao_var
default_universe = '1'
    v1_var%name = 'Lcell'
    default_step = 1e-4
    default_attribute = 'Lcell'
    var(1:)%ele_name = 'O_L'
    default_key_bound = T
    default_key_delta = 0.1
/

